{
  "name": "react-express-starter-pack",
  "version": "1.0.0",
  "description": "Boilerplate for MERN stack with Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && yarn install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Alberto Aragon",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.2.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
